class Solution {
    public static int numberOfTriangles(int n, int[][] g) {
        // code here
        int cnt=0;
        
        for(int i=0 ; i<g.length ; i++){
            for(int j=0 ; j<g.length ; j++){
                for(int k=0 ; k<g.length ; k++){
                    if(i!=j && i!=k && j!=k && g[i][j]==1 && g[j][k]==1 && g[k][i]==1){
                        cnt++;
                    }
                }
            }
        }
        return cnt/3;
    }
}
