/*
Structure of Node class is:

class Node {
    int data;
    Node left, right;

    public Node(int data){
        this.data = data;
    }
}
*/

class Solution {
    public ArrayList<Node> helper(Node root, int n){
        if(root==null)return new ArrayList<>();
        
        if(root.data==n){
            ArrayList<Node> al=new ArrayList<>();
            al.add(root);
            return al;
        }
        
        ArrayList<Node> left=helper(root.left,n);
        if(left.size()>0){
            left.add(root);
            return left;
        }
        
        ArrayList<Node> righ=helper(root.right,n);
        if(righ.size()>0){
            righ.add(root);
            return righ;
        }
        
        return new ArrayList<>();
        
    }
    public int kthAncestor(Node root, int k, int n) {
        // Write your code here
        ArrayList<Node> al=helper(root,n);
        
        if(al.size()<=k)return -1;
        return al.get(k).data;
    }
}
