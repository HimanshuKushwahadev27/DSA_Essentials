/*
class Node
{
    int data;
    Node left, right;

    public Node(int d)
    {
        data = d;
        left = right = null;
    }
}
*/
class Solution {
    int ans;
    void helper(Node root, int l, int h){
        if(root==null)return;
        
        if(root.data>=l && root.data<=h){
            ans++;
        }
        
        helper(root.left,l,h);
        helper(root.right,l,h);
    }
    int getCount(Node root, int l, int h) {
        // Your code here
        ans=0;
        helper(root, l ,h);
        return ans;
    }
}
